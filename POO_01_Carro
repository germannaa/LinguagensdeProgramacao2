
package POO;


public class Carro01 {
    //Atributos
    int qntPortas;
    int qntPneus;
    String cor;
    
    //Método Construtor
    Carro01() {
        System.out.println("Metodo construtor.. Um carro foi instanciado.");
        }
    
    //Métodos
    void acelerar(){
        System.out.println ("Acelerando....");
        }
     void frear(){
        System.out.println ("Freando.........");
        }
}
